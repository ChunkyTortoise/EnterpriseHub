{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "ghl": {
      "command": "npx",
      "args": ["-y", "ghl-mcp-server@latest"],
      "env": {
        "GHL_API_KEY": "${GHL_API_KEY}",
        "GHL_BASE_URL": "https://services.leadconnectorhq.com",
        "GHL_LOCATION_ID": "${GHL_LOCATION_ID}"
      }
    },
    "gumroad": {
      "command": "npx",
      "args": ["-y", "gumroad-mcp@latest"],
      "env": {
        "GUMROAD_ACCESS_TOKEN": "${GUMROAD_ACCESS_TOKEN}"
      }
    },
    "upwork": {
      "command": "npx",
      "args": ["-y", "@chinchillaenterprises/mcp-upwork@latest"]
    },
    "notebooklm": {
      "command": "/Users/cave/Documents/GitHub/EnterpriseHub/.venv_fix/bin/python",
      "args": [".claude/mcp-servers/notebooklm/server.py"]
    },
    "obsidian": {
      "command": "/Users/cave/Documents/GitHub/EnterpriseHub/venv/bin/python",
      "args": [".claude/mcp-servers/obsidian/server.py"],
      "env": {
        "VIRTUAL_ENV": "/Users/cave/Documents/GitHub/EnterpriseHub/venv",
        "PYTHONPATH": "/Users/cave/Documents/GitHub/EnterpriseHub/venv/lib/python3.14/site-packages",
        "OBSIDIAN_VAULT_PATH": "/Users/cave/Documents/GitHub/EnterpriseHub/.obsidian-vault"
      }
    }
  }
}