# Voice AI Platform Environment Variables
# Copy this file to .env and fill in your actual values

# Application
APP_NAME="Voice AI Platform"
DEBUG=false
BASE_URL=your-domain.com

# Security
ALLOWED_ORIGINS=http://localhost:3000
API_KEYS=
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Database (PostgreSQL)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/voice_ai

# Redis (caching and real-time state)
REDIS_URL=redis://localhost:6379/0

# Deepgram STT (Speech-to-Text)
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# ElevenLabs TTS (Text-to-Speech)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_DEFAULT_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# Twilio (telephony)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=+15551234567

# Stripe (billing and payments)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# Anthropic Claude (LLM)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
LLM_MODEL=claude-sonnet-4-5-20250929

# Pipeline Configuration
STT_ENDPOINTING_MS=300
TTS_MODEL=eleven_flash_v2_5
VAD_SILENCE_THRESHOLD_MS=1000

# Billing Rates (per minute)
RATE_PAYG=0.20
RATE_GROWTH=0.15
RATE_ENTERPRISE=0.12
RATE_WHITELABEL=0.10
